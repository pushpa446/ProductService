{
  "consumer": {
    "name": "penguin"
  },
  "provider": {
    "name": "product-service"
  },
  "interactions": [
    {
      "description": "a request for product data",
      "providerState": "HasProductDetails",
      "request": {
        "method": "GET",
        "path": "/products/1234"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json;charset=UTF-8"
        },
        "body": {
          "id": 1234,
          "name": "Shoe",
          "price": 345
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}